{
  "name": "store-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve & npm run svg:watch",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "format": "prettier --write \"./src/**/*.{js,vue,json}\"",
    "svg:watch": "onchange './src/assets/svg/*.svg' -- npm run svg",
    "svg": "vsvg -s ./src/assets/svg -t ./src/assets/icon"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "core-js": "^2.6.5",
    "element-ui": "^2.7.2",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-service": "^3.5.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.9.0",
    "onchange": "^4.1.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21",
    "vue-svgicon": "^2.1.3",
    "vuelidate": "^0.7.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
